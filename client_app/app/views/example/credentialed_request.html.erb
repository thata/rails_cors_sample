<h1>Example#credentialed_request</h1>
<p>Find me in app/views/example/credentialed_request.html.erb</p>

<script type="text/javascript">
    function callOtherDomain() {
        $.ajax({
            url: 'http://localhost:3000/resources/access_control_with_credential',
            xhrFields: {
                withCredentials: true
            }
        }).success(function(res){
            var invocationHistoryText = document.createTextNode("count: " + res['count']);
            var textDiv = document.getElementById("textDiv");
            textDiv.appendChild(invocationHistoryText);
        })
    }
</script>

<form id="controlsToInvoke" action="">
    <p>
    <input type="button" value="Click to Invoke Another Site" onclick="callOtherDomain()" />
    </p>
</form>

<div id="textDiv" />
